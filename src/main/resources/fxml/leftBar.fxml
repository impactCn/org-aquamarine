<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<VBox  fx:id="leftBarContainer" maxWidth="-Infinity" minWidth="-Infinity"
       prefWidth="40.0"  style="-fx-background-color: #3c3f41"
       styleClass="leftBarContainer"
       xmlns="http://javafx.com/javafx/20.0.1"
       xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.impactcn.aquamarine.controller.LeftBarController">
   <HBox fx:id="topPart" alignment="TOP_CENTER"
         style="-fx-background-color: #3c3f41;" VBox.vgrow="ALWAYS"  >
      <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" styleClass="fold" fx:id="foldBtn">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Button>
   </HBox>
   <HBox fx:id="buttonPart" alignment="BOTTOM_CENTER"     style="-fx-background-color: #3c3f41;" VBox.vgrow="ALWAYS" >
      <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" styleClass="setting" fx:id="settingBtn">
         <HBox.margin>
            <Insets bottom="5.0" />
         </HBox.margin>
      </Button>
   </HBox>
</VBox>
